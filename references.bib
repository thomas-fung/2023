@article{senetaSimpleStepwiseTests2005,
  title = {Simple {{Stepwise Tests}} of {{Hypotheses}} and {{Multiple Comparisons}}},
  author = {Seneta, Eugene and Chen, John T.},
  year = {2005},
  journal = {International Statistical Review / Revue Internationale de Statistique},
  volume = {73},
  number = {1},
  eprint = {25472638},
  eprinttype = {jstor},
  pages = {21--34},
  publisher = {{[Wiley, International Statistical Institute (ISI)]}},
  issn = {0306-7734},
  urldate = {2021-07-29},
  abstract = {Holm's (1979) step-down and Hochberg's (1988) step-up procedures for tests of multiple hypotheses are simple to apply and are widely used. Holm's procedure controls the familywise error rate (FWE), while Hochberg's is more powerful. This paper investigates a step-down procedure (labelled CS) of Seneta \& Chen (1997) which is a sharpening of Holm's, takes into account the degree of association between test statistics, and also controls the FWE. Computation for the CS procedure may be minimized by using the procedure as an adjustment to Holm's. The computational steps are detailed, and the adjustment is then illustrated by an application to a text-book example of multiple comparisons, in which step-wise procedures are shown to perform better than the usual Tukey T-comparison. Simulation investigations in a standard comparison with a control setting show that the CS step-down procedure is more powerful than Hochberg's step-up procedure and the procedure of Simes (1986), especially in regard to error rate, and not much less powerful than an optimal, but very specific, step-up procedure of Dunnett \& Tamhane (1992). /// Les proc\'edures descendante de Holm (1979) et montante de Hochberg (1988) pour les tests d'hypoth\`eses multiples sont faciles \`a appliquer et d'usage courant. La proc\'edure de Holm contr\^ole le taux d'erreur par famille ("familywise error rate", FWE), tandis que celle de Hochberg est plus puissante. Cet article examine une proc\'edure descendante ("CS") de Seneta \& Chen (1997) qui affine celle de Holm, prend en compte le degr\'e d'association entre tests statistiques et contr\^ole le taux FWE. On peut minimiser les calculs pour CS en utilisant cette proc\'edure comme un ajustement de celle de Holm. Les d\'etails des \'etapes de calcul sont explicit\'es; puis l'ajustement est illustr\'e par une application \`a un exemple de comparisons multiples se trouvant dans les manuels: on y montre que les proc\'edures pas \`a pas sont plus performantes que la T-comparaison usuelle de Tukey. Gr\^ace \`a des recherches par simulation sur une comparaison standard, avec un dispositif de contr\^ole, on montre ceci: la proc\'edure descendante CS est plus puissante que la proc\'edure montante de Hochberg et que celle de Simes (1986), en particulier quant au taux d'erreur; en outre elle n'est pas beaucoup moins puissante que la proc\'edure montante optimale, mais tr\`es sp\'ecifique, de Dunnett \& Tamhane (1992).},
  keywords = {gh\_rate,sarkar},
  file = {/Users/thomasfung/Documents/Zotero/storage/HIEC5I3S/Seneta and Chen - 2005 - Simple Stepwise Tests of Hypotheses and Multiple C.pdf}
}


